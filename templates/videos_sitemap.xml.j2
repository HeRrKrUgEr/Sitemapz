<?xml version="1.0" encoding="UTF-8"?>
<urlset 
  xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
  xmlns:video="http://www.google.com/schemas/sitemap-video/1.1">
{% for url in urls %}
  <url>
    <loc>{{ url }}</loc>
    <video:video>
      <video:content_loc>{{ url }}</video:content_loc>
    </video:video>
  </url>
{% endfor %}
</urlset>
